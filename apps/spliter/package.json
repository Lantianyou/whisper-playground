    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-electron-renderer": "^0.14.1"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.0",
    "@ffmpeg/util": "^0.12.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
